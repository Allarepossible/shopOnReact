    "npm-run-all": "4.1.1",
    "polished": "^2.3.0",
    "ramda": "^0.25.0",
    "react": "16.6.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "5.0.6",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "3.7.2",
