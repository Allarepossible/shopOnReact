{
  "name": "shop-react",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "lint": "./node_modules/.bin/eslint src",
    "lint-fixup": "./node_modules/.bin/eslint src --ext .js,.jsx --fix",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch dist --exec \"babel-node dist/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "webpack-devserver": "webpack-dev-server --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8090 --host 0.0.0.0"
  },
  "dependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.0.9",
    "axios": "0.16.2",
    "body-parser": "^1.18.3",
    "clean-tag": "^2.0.2",
    "cors": "^2.8.5",
    "express": "4.15.4",
    "express-http-proxy": "^1.5.1",
    "history": "^4.7.2",
    "idempotent-babel-polyfill": "^7.0.0",
    "lodash": "4.17.4",
    "mongoose": "^5.3.12",
    "npm-run-all": "4.1.1",
    "polished": "^2.3.0",
    "ramda": "^0.25.0",
    "react": "16.6.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "5.0.6",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "3.7.2",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "^1.7.0",
    "styled-components": "^4.0.3",
    "styled-system": "^3.1.11",
    "url-loader": "^1.1.2",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.6",
    "qs": "^6.6.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.5.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
