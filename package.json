    "babel-eslint": "^9",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
