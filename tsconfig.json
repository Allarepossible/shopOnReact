{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "checkJs": false,
    "jsx": "react",
    "outDir": "./build",
    "removeComments": true,
    "noEmit": true,
    "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "module": "es2015",
    "lib": [
      "es2015",
      "es2016"
    ],
    "esModuleInterop": true
  },
  "include": [
    "./src/client/client.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
